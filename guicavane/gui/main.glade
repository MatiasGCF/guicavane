  </object>
  <object class="GtkListStore" id="nameListstore">
    <columns>
      <!-- column-name name -->
      <column type="gchararray"/>
    </columns>
  </object>
  <object class="GtkMenu" id="nameShowsMenu">
    <property name="visible">True</property>
    <property name="can_focus">False</property>
    <child>
      <object class="GtkImageMenuItem" id="addfavoriteItem">
        <property name="label" translatable="yes">Add to favorites</property>
        <property name="visible">True</property>
        <property name="can_focus">False</property>
        <property name="use_action_appearance">False</property>
        <property name="image">imageAdd</property>
        <property name="use_stock">False</property>
        <property name="always_show_image">True</property>
        <signal name="activate" handler="_on_name_add_favorite" swapped="no"/>
      </object>
    </child>
